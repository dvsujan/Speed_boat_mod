{
    "schemaVersion": 1,
    "id": "modid",
    "version": "${version}",

    "name": "Example Mod",
    "description": "This is an example description! Tell everyone what your mod is about!",
    "authors": [
        "dvsujan"
    ],
    "contact": {
        "homepage": "https://dvsujan.github.io/"
    },

    "license": "CC0-1.0",
    "icon": "assets/modid/icon.png",

    "environment": "*",
    "entrypoints": {
        "main": [
            "net.fabricmc.example.ExampleMod"
        ],
        "hacks": [
            "net.fabricmc.example.hacks.FlyHack",
            "net.frabicmc.example.hacks.SpeedHack"
        ]
    },
    "mixins": [
        "modid.mixins.json"
    ],


    "depends": {
        "fabricloader": ">=0.14.6",
        "fabric": "*",
        "minecraft": "~1.19",
        "java": ">=17"
    },
    "suggests": {
        "another-mod": "*"
    }
}